{
  "name": "adonis-body-parser",
  "version": "1.0.0",
  "description": "Body parser middleware for Adonis 4.0 and above",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "posttest": "npm run coverage",
    "test:local": "FORCE_COLOR=true node bin/index.js --local",
    "test": "./node_modules/.bin/nyc npm run test:local",
    "test:win": "set FORCE_COLOR=true && node bin/index.js --win",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "standard": {
    "global": [
      "use",
      "it",
      "describe",
      "context",
      "before",
      "after"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-middleware.git"
  },
  "keywords": [
    "adonis",
    "http-middleware",
    "middleware",
    "adonisjs"
  ],
  "author": "adonisjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-middleware/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-middleware#readme",
  "dependencies": {
    "bytes": "^2.5.0",
    "co-body": "^5.1.1",
    "debug": "^2.6.8",
    "end-of-stream": "^1.4.0",
    "fs-extra": "^3.0.1",
    "get-stream": "^3.0.0",
    "multiparty": "^4.1.3",
    "node-exceptions": "^2.0.1"
  },
  "devDependencies": {
    "adonis-sink": "git+https://github.com/adonisjs/adonis-sink.git#develop",
    "coveralls": "^2.13.1",
    "cz-conventional-changelog": "^2.0.0",
    "japa": "^1.0.3",
    "japa-cli": "^1.0.1",
    "media-typer": "^0.3.0",
    "node-req": "git+https://github.com/poppinss/node-req.git#develop",
    "nyc": "^10.3.2",
    "semver": "^5.3.0",
    "standard": "^10.0.2",
    "supertest": "^3.0.0",
    "zombie": "^5.0.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "src"
    ]
  }
}
