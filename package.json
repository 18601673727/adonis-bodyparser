{
  "name": "@adonisjs/bodyparser",
  "version": "2.0.2",
  "description": "Body parser middleware for Adonis 4.0 and above",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "posttest": "npm run coverage",
    "test:local": "FORCE_COLOR=true node bin/index.js --local",
    "test": "./node_modules/.bin/nyc npm run test:local",
    "test:win": "set FORCE_COLOR=true && node bin/index.js --win",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "standard": {
    "global": [
      "use",
      "it",
      "describe",
      "context",
      "before",
      "after"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-bodyparser.git"
  },
  "keywords": [
    "adonis",
    "http-middleware",
    "middleware",
    "adonisjs"
  ],
  "author": "adonisjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-bodyparser/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-bodyparser#readme",
  "dependencies": {
    "@adonisjs/generic-exceptions": "^2.0.0",
    "bytes": "^3.0.0",
    "co-body": "^5.1.1",
    "debug": "^3.1.0",
    "end-of-stream": "^1.4.1",
    "fs-extra": "^5.0.0",
    "get-stream": "^3.0.0",
    "lodash": "^4.17.5",
    "multiparty": "^4.1.3"
  },
  "devDependencies": {
    "@adonisjs/fold": "^4.0.5",
    "@adonisjs/sink": "^1.0.13",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "japa": "^1.0.6",
    "japa-cli": "^1.0.1",
    "media-typer": "^0.3.0",
    "node-req": "^2.1.0",
    "nyc": "^11.4.1",
    "semver": "^5.5.0",
    "standard": "^10.0.3",
    "supertest": "^3.0.0",
    "zombie": "^5.0.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "src"
    ]
  }
}
